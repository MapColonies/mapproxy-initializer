  "DB": {
    "host": "DB_HOST",
    "database": "DB_NAME",
    "user": "DB_USER",
    "password": "DB_PASSWORD",
    "port": "DB_PORT",
    "sslEnabled": "DB_SSL_ENABLED",
    "rejectUnauthorized": "DB_REJECT_UNAUTHORIZED",
